	$(V) -o big_numbers testbench_big_numbers.v ADD4.v ALU.v CONTROL.v DECODER.v DECODER_REG.v INSTR_MEM.v MUX2.v MUX4.v MUX5.v PC.v REG_FILE.v ITYPESIGNEXTEND.v UTYPE.v BRANCHCONDGEN.v BRANCHTARGGEN.v DATAMEM.v STYPESIGNEXTEND.v JUMPREGTARGGEN.v JUMPTARGGEN.v

testbench: testbench.v ADD4.v ALU.v CONTROL.v DECODER.v DECODER_REG.v INSTR_MEM.v MUX2.v MUX4.v MUX5.v PC.v REG_FILE.v ITYPESIGNEXTEND.v UTYPE.v BRANCHCONDGEN.v BRANCHTARGGEN.v DATAMEM.v STYPESIGNEXTEND.v JUMPREGTARGGEN.v JUMPTARGGEN.v
	$(V) -o testbench testbench.v ADD4.v ALU.v CONTROL.v DECODER.v DECODER_REG.v INSTR_MEM.v MUX2.v MUX4.v MUX5.v PC.v REG_FILE.v ITYPESIGNEXTEND.v UTYPE.v BRANCHCONDGEN.v BRANCHTARGGEN.v DATAMEM.v STYPESIGNEXTEND.v JUMPREGTARGGEN.v JUMPTARGGEN.v


