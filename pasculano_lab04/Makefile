
EXECS = multiply big_numbers testbench

all: $(EXEC)

# THESE HAVE NOT BEEN TESTED
EXTENDERS = ITYPESIGNEXTEND.v UTYPE.v STYPESIGNEXTEND.v
MUX = MUX2.v MUX4.v MUX5.v
DECODE = DECODER.v DECODER_REG.v
BRANCH = BRANCHCONDGEN.v BRANCHTARGGEN.v JUMPREGTARGGEN.v JUMPTARGGEN.v

V=iverilog

